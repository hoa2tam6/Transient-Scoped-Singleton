@{
    ViewData["Title"] = "Home Page";
}


<div class="text-center">  
    <h2 class="display-4">Dependency Injection Lifetime  
    </h2>  
</div>  
<table class="table table-bordered">  
    <thead>  
        <tr>  
            <th>Service Type</th>  
            <th>First Instance Operation ID</th>  
            <th>Second Instance Operation ID</th>  
            <th>Is Changed?</th>  
        </tr>  
    </thead>  
    <tbody>  
        <tr>  
            <td style="background-color: darksalmon">Transient  
            </td>  
            <td style="background-color: darksalmon">@ViewBag.transient1  
            </td>  
            <td style="background-color: darksalmon">@ViewBag.transient2  
            </td>  
            <td style="background-color: darksalmon">@ViewBag.TransientIsChange</td>
        </tr>  
        <tr>  
            <td>Scoped</td>  
            <td>@ViewBag.scoped1</td>  
            <td>@ViewBag.scoped2</td>  
            <td>@ViewBag.ScopedIsChange</td>
        </tr>  
        <tr>  
            <td style="background-color: aquamarine">Singleton  
            </td>  
            <td style="background-color: aquamarine">@ViewBag.singleton1  
            </td>  
            <td style="background-color: aquamarine">@ViewBag.singleton2  
            </td>  
            <td style="background-color: aquamarine">
                @ViewBag.SingletonIsChange
            </td>
        </tr>  
    </tbody>  
</table>  